{% raw %}
{
	"title": {{this.name | json }},
	"start": "{{this.properties.module_field_12_2 | date: "%Y-%m-%dT%H:%M:%SZ"}}",
	"end": "{{this.properties.module_field_12_3 | date: "%Y-%m-%dT%H:%M:%SZ"}}",
	"url": "{{this.full_slug}}",
	"classNames": [
		{%- for category in this.category_array -%}
			"events-category-{{category}}"{%- unless forloop.last -%},{%- endunless -%}
		{%- endfor -%}
	]
}{%- unless forloop.last -%},{%- endunless -%}
{% endraw %}