{% raw %}
<div class="col-12 col-md-6">
  <div class="bg-white rounded-card mb-4 p-5">
    <div class="row">
      <div class="col-12 col-md-3 mb-5">
        <img class="rounded-circle mw-100" src="{{ this['Image'] | asset_url }}" alt="{{this['name']}}">
      </div>
      <div class="col-12 col-md-8 offset-md-1">
          <p>"{{this['Quote']}}"</p>
          <p class="fw-bold mb-1">{{this['name']}}</p>
          <p class="fw-bold">{{this['Job Title']}}, {{this['Company Name']}}</p>
      </div>
    </div>
  </div>
</div>
{% endraw %}