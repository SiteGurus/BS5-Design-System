{%- raw -%}
{% comment %} ---Settings--- {% endcomment %}
  {% comment %}------ Sign up URL ------{% endcomment %}
  {% assign sign_up_url = "/sign-up" %}
  {% comment %}------ Menu Title 1 ------{% endcomment %}
  {% assign menu_title_1 = "About" %}
  {% comment %}------ Menu Link 1 ------{% endcomment %}
  {% assign menu_link_1 = "/about" %}
  {% comment %}------ Menu Title 2 ------{% endcomment %}
  {% assign menu_title_2 = "Terms and Conditions" %}
  {% comment %}------ Menu Link 2 ------{% endcomment %}
  {% assign menu_link_2 = "/terms-and-conditions" %}
  {% comment %}------ Menu Title 3 ------{% endcomment %}
  {% assign menu_title_3 = "Contact" %}
  {% comment %}------ Menu Link 3 ------{% endcomment %}
  {% assign menu_link_3 = "/contact" %}
{% comment %}---End Settings--- {% endcomment %}
{% capture sitegurus_ui_uniq_component_id %}sitegurus_component_{% increment sitegurus_gen_uniq_component_id %}{% endcapture %}

<section class="bg-gray-50 dark:bg-gray-900">
  <div class="max-w-screen-xl px-4 py-8 mx-auto lg:grid lg:gap-20 lg:py-16 lg:grid-cols-12">
    <div class="flex-col justify-between hidden col-span-6 mr-auto lg:flex xl:mb-0">
      <div>
        <a href="/" class="inline-flex items-center mb-6 text-2xl font-semibold text-gray-900 lg:mb-10 dark:text-white">
          <img class="w-48 mr-2 dark:hidden" src="{% if context.exports.company_information.properties.logo != blank %}{{context.exports.company_information.properties.logo | asset_url}}{% else %}https://flowbite.com/docs/images/logo.svg{% endif %}" alt="{{context.exports.company_information.properties.company_name}} logo">
          <img class="w-48 mr-2 hidden dark:block" src="{% if context.exports.company_information.properties.logo_white != blank %}{{context.exports.company_information.properties.logo_white | asset_url}}{% else %}https://flowbite.com/docs/images/logo.svg{% endif %}" alt="{{context.exports.company_information.properties.company_name}} logo">
          <span class="hidden">{{context.exports.company_information.properties.company_name}}</span>
        </a>
        <div class="flex">
          <svg class="w-5 h-5 mr-2 text-primary-600 shrink-0" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path></svg>
          <div>
            <h3 class="mb-2 text-xl font-bold leading-none text-gray-900 dark:text-white">Get started quickly</h3>
            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">Integrate with developer-friendly APIs or choose low-code.</p>
          </div>
        </div>
        <div class="flex pt-8">
          <svg class="w-5 h-5 mr-2 text-primary-600 shrink-0" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path></svg>
          <div>
            <h3 class="mb-2 text-xl font-bold leading-none text-gray-900 dark:text-white">Support any business model</h3>
            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">Host code that you don't want to share with the world in private.</p>
          </div>
        </div>
        <div class="flex pt-8">
          <svg class="w-5 h-5 mr-2 text-primary-600 shrink-0" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"></path></svg>
          <div>
            <h3 class="mb-2 text-xl font-bold leading-none text-gray-900 dark:text-white">Join millions of businesses</h3>
            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">Flowbite is trusted by ambitious startups and enterprises of every size.</p>
          </div>
        </div>
      </div>
      <nav>
        <ul class="flex space-x-4">
          <li>
            <a href="{{menu_link_1}}" class="text-sm text-gray-500 hover:underline hover:text-gray-900 dark:text-gray-400 dark:hover:text-white">{{menu_title_1}}</a>
          </li>
          <li>
            <a href="{{menu_link_2}}" class="text-sm text-gray-500 hover:underline hover:text-gray-900 dark:text-gray-400 dark:hover:text-white">{{menu_title_2}}</a>
          </li>
          <li>
            <a href="{{menu_link_3}}" class="text-sm text-gray-500 hover:underline hover:text-gray-900 dark:text-gray-400 dark:hover:text-white">{{menu_title_3}}</a>
          </li>
        </ul>
      </nav>
    </div>
    <div class="mb-6 text-center lg:hidden">
      <a href="#" class="inline-flex items-center text-2xl font-semibold text-gray-900 lg:hidden dark:text-white">
        <img class="w-8 h-8 mr-2" src="{% if context.exports.company_information.properties.logo != blank %}{{context.exports.company_information.properties.logo | asset_url}}{% else %}https://flowbite.s3.amazonaws.com/blocks/marketing-ui/logo.svg{% endif %}" alt="{{context.exports.company_information.properties.company_name}} logo">
        <span class="hidden">{{context.exports.company_information.properties.company_name}}</span>
      </a>
    </div>
    <div class="w-full col-span-6 mx-auto bg-white rounded-lg shadow dark:bg-gray-800 md:mt-0 sm:max-w-lg xl:p-0">
      <div class="p-6 space-y-4 lg:space-y-6 sm:p-8">
        {% if context.current_user.id == blank %}
          <h1 class="text-xl font-bold leading-tight tracking-tight text-gray-900 sm:text-2xl dark:text-white">Welcome back</h1>
          {% form html_class: 'space-y-4 lg:space-y-6' -%}
            <div>
              <label for="email" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Email</label>
              <input id="email" name="{{ form_builder.fields.email.name }}" class="bg-gray-50 border border-gray-300 text-gray-900 sm:text-sm rounded-lg focus:ring-primary-600 focus:border-primary-600 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" type="email" placeholder="john@example.com" autofill="email">
            </div>
            <div>
              <label for="password" class="block mb-2 text-sm font-medium text-gray-900 dark:text-white">Password</label>
              <input id="password" class="bg-gray-50 border border-gray-300 text-gray-900 sm:text-sm rounded-lg focus:ring-primary-600 focus:border-primary-600 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" type="password" placeholder="Password" name="{{ form_builder.fields.password.name }}" autofill="existing_password" aria-label="password">
            </div>
            <div class="hidden p-4 mb-4 text-sm text-danger-700 bg-danger-100 rounded-lg dark:bg-danger-200 dark:text-danger-800 form-error" role="alert">
              <!-- Siteglide error message goes here -->
            </div>
            <div class="flex items-center justify-between">
              <a href="/system/recover-password" class="text-sm font-medium text-primary-600 hover:underline dark:text-primary-500">Forgot password?</a>
            </div>
            <button class="w-full text-white bg-primary-600 hover:bg-primary-700 focus:ring-4 focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 text-center dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800" onclick="siteglide_login_process(this,'{{_redirect}}','{{_empty_error_msg}}')">Sign in to your account</button>
            <p class="text-sm font-light text-gray-500 dark:text-gray-400">Donâ€™t have an account yet? <a href="{{sign_up_url}}" class="font-medium text-primary-600 hover:underline dark:text-primary-500">Sign up here</a></p>
          {% endform -%}
        {% else %}
          <div class="p-4 mb-4 text-sm text-warning-700 bg-warning-100 rounded-lg dark:bg-warning-200 dark:text-warning-800" role="alert">
            <span class="font-medium">You're already logged in.</span>
          </div>
          <a class="text-white bg-primary-700 hover:bg-primary-800 focus:ring-4 focus:outline-none focus:ring-primary-300 font-medium rounded-lg text-sm w-full sm:w-auto px-5 py-2.5 text-center dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800 mt-4" onclick="history.back()" href="javascript: void(0);">Back</a>
        {% endif %}
      </div>
    </div>              
  </div>
</section>
{%- endraw -%}